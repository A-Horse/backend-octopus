{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/node_modules/.bin/ts-node",
      "args": ["src/main.ts"],
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "/usr/bin/node",
      "windows": {
        "outFiles": ["${workspaceFolder}\\**\\*.js"],
        "sourceMaps": true,
        "program": "${workspaceFolder}\\node_modules\\.bin\\ts-node",
        "args": ["src\\main.ts"]
      }
    }
  ]
}
